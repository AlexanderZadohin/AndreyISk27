        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AndreyISk27.View.Windows"
        mc:Ignorable="d"
        Title="MainAppWindow" Height="768" Width="1366" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45*"/>
            <RowDefinition Height="389*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="41*"/>
            <ColumnDefinition Width="159*"/>
        </Grid.ColumnDefinitions>
        <Frame Grid.Row="1" Name="MenuFrm"/>
        <Frame Grid.Column="1" Grid.Row="1" Name="MainBodyFrm"/>
        <Grid Background="DarkGray" Grid.ColumnSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="565*"/>
                <ColumnDefinition Width="177*"/>
                <ColumnDefinition Width="58*"/>
            </Grid.ColumnDefinitions>

        </Grid>
    </Grid>
</Window>
